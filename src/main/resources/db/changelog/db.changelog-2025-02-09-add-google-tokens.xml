<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2025-02-09-01" author="leonid">
        <addColumn tableName="users">
            <column name="google_access_token" type="TEXT"/>
            <column name="google_refresh_token" type="TEXT"/>
            <column name="google_token_expiry" type="TIMESTAMP"/>
            <column name="google_resource_name" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>